#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>
#define MAX (100001)

int answer[MAX];
int* solution(int prices[], size_t prices_len) {
    
    //int* answer = (int*)malloc(1); // return 값은 malloc 등 동적 할당을 사용(할당 길이는 상황에 맞게 변경)
    
    for(int i = 0; i < prices_len; i++) // i -> 0, 1, 2, 3, 4
    {
        for(int j = (i + 1); j < prices_len; j++) // j -> 1, 2, 3, 4
        {        
            answer[i]++;
        
            if(prices[i] > prices[j]) break;
        }
    }
    return answer;
}
